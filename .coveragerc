[run]
source = .
omit =
    */tests/*
    */migrations/*
    setup.py
    __init__.py
    */venv/*
    */.venv/*
    wsgi.py
    manage.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    skipif.*\[requires|need\]

[html]
directory = htmlcov

[xml]
output = coverage.xml